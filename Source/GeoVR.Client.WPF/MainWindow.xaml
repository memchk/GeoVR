        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoVR.Client.WPF"
        mc:Ignorable="d"
        Title="GeoVR Voice Code 2 - Proof Of Concept" Height="486" Width="387.317" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp" LostFocus="Window_LostFocus" Deactivated="Window_Deactivated">
